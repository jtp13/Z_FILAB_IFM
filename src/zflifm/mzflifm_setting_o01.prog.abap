MODULE status_0100 OUTPUT.

  SET PF-STATUS '0100'.
  SET TITLEBAR '0100'.

ENDMODULE.                 " STATUS_0100  OUTPUT
*&---------------------------------------------------------------------*
*&      Module  SET_DATA  OUTPUT
*&---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE set_data OUTPUT.
  IF gv_init IS INITIAL.
    PERFORM get_data.
    gv_init = 'X'.
  ENDIF.
ENDMODULE.                 " SET_DATA  OUTPUT
*&---------------------------------------------------------------------*
*&      Module  SET_CURSOR_FIELD  OUTPUT
*&---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE set_cursor_field OUTPUT.

  SET CURSOR FIELD gv_cursor_field  LINE gv_cursor_line .

ENDMODULE.                 " SET_CURSOR_FIELD  OUTPUT
